{
  "mcpServers": {
    "playwright": {
      "disabled": true,
      "timeout": 60,
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    },

    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "disabled": true,
      "alwaysAllow": []
    }
  }
}
